CREATE TABLE IF NOT EXISTS `tbl_admin_posting` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `img` text,
  `posting` text NOT NULL,
  `posting_date` datetime NOT NULL,
  `posting_date_ts` int(11) NOT NULL,
  `n_status` int(11) NOT NULL,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB  DEFAULT CHARSET=latin1 AUTO_INCREMENT=5 ;
CREATE TABLE IF NOT EXISTS `tbl_admin_posting` (   `id` int(11) NOT NULL AUTO_INCREMENT,   `img` text,   `posting` text NOT NULL,   `posting_date` datetime NOT NULL,   `posting_date_ts` int(11) NOT NULL,   `n_status` int(11) NOT NULL,   PRIMARY KEY (`id`) ) ENGINE=InnoDB  DEFAULT CHARSET=latin1 AUTO_INCREMENT=5;
