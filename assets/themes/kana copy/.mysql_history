DROP TABLE IF EXISTS `axis_news_content`;
CREATE TABLE IF NOT EXISTS `axis_news_content` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `parentid` int(11) NOT NULL,
  `lid` int(11) NOT NULL,
  `title` varchar(200) NOT NULL,
  `brief` varchar(200) NOT NULL,
  `content` text NOT NULL,
  `image` varchar(200) NOT NULL,
  `categoryid` int(11) NOT NULL,
  `articleType` int(11) NOT NULL,
  `prize` bigint(20) NOT NULL DEFAULT '0',
  `url` varchar(200) NOT NULL,
  `created_date` datetime NOT NULL,
  `expired_date` datetime NOT NULL,
  `posted_date` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
  `online` int(11) NOT NULL DEFAULT '0' COMMENT '1: iphone, 2:android; 3:bb ; 4: feature phone',
  `n_status` int(11) NOT NULL DEFAULT '0',
  PRIMARY KEY (`id`),
  UNIQUE KEY `IDX_parent_lid` (`parentid`,`lid`),
  KEY `title` (`title`),
  KEY `brief` (`brief`),
  KEY `categoryid` (`categoryid`),
  KEY `created_date` (`created_date`),
  KEY `posted_date` (`posted_date`),
  KEY `n_status` (`n_status`),
  KEY `articleTypeID` (`articleType`),
  KEY `image` (`image`),
  KEY `parentID` (`parentid`),
  KEY `lid` (`lid`),
  KEY `online` (`online`),
  KEY `expired_date` (`expired_date`),
  KEY `prize` (`prize`),
  KEY `url` (`url`)
) ENGINE=InnoDB  DEFAULT CHARSET=latin1 AUTO_INCREMENT=47 ;
CREATE TABLE IF NOT EXISTS `axis_news_content` (   `id` int(11) NOT NULL AUTO_INCREMENT,   `parentid` int(11) NOT NULL,   `lid` int(11) NOT NULL,   `title` varchar(200) NOT NULL,   `brief` varchar(200) NOT NULL,   `content` text NOT NULL,   `image` varchar(200) NOT NULL,   `categoryid` int(11) NOT NULL,   `articleType` int(11) NOT NULL,   `prize` bigint(20) NOT NULL DEFAULT '0',   `url` varchar(200) NOT NULL,   `created_date` datetime NOT NULL,   `expired_date` datetime NOT NULL,   `posted_date` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,   `online` int(11) NOT NULL DEFAULT '0' COMMENT '1: iphone, 2:android; 3:bb ; 4: feature phone',   `n_status` int(11) NOT NULL DEFAULT '0',   PRIMARY KEY (`id`),   UNIQUE KEY `IDX_parent_lid` (`parentid`,`lid`),   KEY `title` (`title`),   KEY `brief` (`brief`),   KEY `categoryid` (`categoryid`),   KEY `created_date` (`created_date`),   KEY `posted_date` (`posted_date`),   KEY `n_status` (`n_status`),   KEY `articleTypeID` (`articleType`),   KEY `image` (`image`),   KEY `parentID` (`parentid`),   KEY `lid` (`lid`),   KEY `online` (`online`),   KEY `expired_date` (`expired_date`),   KEY `prize` (`prize`),   KEY `url` (`url`) ) ENGINE=InnoDB  DEFAULT CHARSET=latin1 AUTO_INCREMENT=47;
