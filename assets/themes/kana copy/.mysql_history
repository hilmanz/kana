DROP TABLE IF EXISTS `social_bookmark`;
CREATE TABLE IF NOT EXISTS `social_bookmark` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `title` varchar(200) NOT NULL,
  `categoryid` int(11) NOT NULL,
  `userid` int(11) NOT NULL,
  `url` varchar(200) NOT NULL,
  `date` datetime NOT NULL,
  `n_status` int(11) NOT NULL DEFAULT '1',
  PRIMARY KEY (`id`),
  KEY `userid` (`userid`),
  KEY `url` (`url`),
  KEY `date` (`date`),
  KEY `n_status` (`n_status`),
  KEY `title` (`title`),
  KEY `categoryid` (`categoryid`)
) ENGINE=InnoDB  DEFAULT CHARSET=latin1 AUTO_INCREMENT=2 ;
CREATE TABLE IF NOT EXISTS `social_bookmark` (   `id` int(11) NOT NULL AUTO_INCREMENT,   `title` varchar(200) NOT NULL,   `categoryid` int(11) NOT NULL,   `userid` int(11) NOT NULL,   `url` varchar(200) NOT NULL,   `date` datetime NOT NULL,   `n_status` int(11) NOT NULL DEFAULT '1',   PRIMARY KEY (`id`),   KEY `userid` (`userid`),   KEY `url` (`url`),   KEY `date` (`date`),   KEY `n_status` (`n_status`),   KEY `title` (`title`),   KEY `categoryid` (`categoryid`) ) ENGINE=InnoDB  DEFAULT CHARSET=latin1 AUTO_INCREMENT=2;
