CREATE TABLE IF NOT EXISTS `tbl_food_image` (
  `user_id` int(11) NOT NULL,
  `image` varchar(200) NOT NULL,
  `n_status` int(11) NOT NULL DEFAULT '1',
  KEY `user_id` (`user_id`,`image`,`n_status`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1;
CREATE TABLE IF NOT EXISTS `tbl_food_image` (   `user_id` int(11) NOT NULL,   `image` varchar(200) NOT NULL,   `n_status` int(11) NOT NULL DEFAULT '1',   KEY `user_id` (`user_id`,`image`,`n_status`) ) ENGINE=InnoDB DEFAULT CHARSET=latin1;
