SELECT a.friend_id as fb_id,b.user_id,a.friend_name 
FROM axis.tbl_fb_friends a
INNER JOIN axis.tbl_user_fb b
ON a.friend_id = b.fb_id
INNER JOIN axis.tbl_online c
ON b.user_id = c.user_id
WHERE a.fb_id = {$fb_id} 
AND c.is_online=1;
SELECT a.friend_id as fb_id,b.user_id,a.friend_name  FROM axis.tbl_fb_friends a INNER JOIN axis.tbl_user_fb b ON a.friend_id = b.fb_id INNER JOIN axis.tbl_online c ON b.user_id = c.user_id WHERE a.fb_id = {$fb_id}  AND c.is_online=1;
SELECT a.friend_id as fb_id,b.user_id,a.friend_name  FROM axis.tbl_fb_friends a INNER JOIN axis.tbl_user_fb b ON a.friend_id = b.fb_id INNER JOIN axis.tbl_online c ON b.user_id = c.user_id WHERE a.fb_id = 1306477325  AND c.is_online=1;
