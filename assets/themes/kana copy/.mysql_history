DROP TABLE IF EXISTS `video_raw`;
CREATE TABLE IF NOT EXISTS `video_raw` (
  `date` date NOT NULL,
  `region` varchar(10) NOT NULL,
  `videoID` varchar(100) NOT NULL,
  `title` varchar(255) NOT NULL,
  `views` float NOT NULL DEFAULT '0',
  `unique_users` float NOT NULL DEFAULT '0',
  `unique_users_7` float NOT NULL DEFAULT '0',
  `unique_users_30` float NOT NULL DEFAULT '0',
  `popularity` float NOT NULL DEFAULT '0',
  `comments` float NOT NULL DEFAULT '0',
  `favorites` float NOT NULL DEFAULT '0',
  `rate1` float NOT NULL DEFAULT '0',
  `rate2` float NOT NULL DEFAULT '0',
  `rate3` float NOT NULL DEFAULT '0',
  `rate4` float NOT NULL DEFAULT '0',
  `rate5` float NOT NULL DEFAULT '0',
  `subscriptions` float NOT NULL DEFAULT '0',
  `unsubscriptions` float NOT NULL DEFAULT '0',
  UNIQUE KEY `uniique_key` (`date`,`region`,`videoID`),
  KEY `unsubscriptions` (`unsubscriptions`),
  KEY `subscriptions` (`subscriptions`),
  KEY `views` (`views`),
  KEY `comments` (`comments`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1;
CREATE TABLE IF NOT EXISTS `video_raw` (   `date` date NOT NULL,   `region` varchar(10) NOT NULL,   `videoID` varchar(100) NOT NULL,   `title` varchar(255) NOT NULL,   `views` float NOT NULL DEFAULT '0',   `unique_users` float NOT NULL DEFAULT '0',   `unique_users_7` float NOT NULL DEFAULT '0',   `unique_users_30` float NOT NULL DEFAULT '0',   `popularity` float NOT NULL DEFAULT '0',   `comments` float NOT NULL DEFAULT '0',   `favorites` float NOT NULL DEFAULT '0',   `rate1` float NOT NULL DEFAULT '0',   `rate2` float NOT NULL DEFAULT '0',   `rate3` float NOT NULL DEFAULT '0',   `rate4` float NOT NULL DEFAULT '0',   `rate5` float NOT NULL DEFAULT '0',   `subscriptions` float NOT NULL DEFAULT '0',   `unsubscriptions` float NOT NULL DEFAULT '0',   UNIQUE KEY `uniique_key` (`date`,`region`,`videoID`),   KEY `unsubscriptions` (`unsubscriptions`),   KEY `subscriptions` (`subscriptions`),   KEY `views` (`views`),   KEY `comments` (`comments`) ) ENGINE=InnoDB DEFAULT CHARSET=latin1;
show tables;
