CREATE TABLE IF NOT EXISTS `fb_page_comment_post` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `comment_id` varchar(200) NOT NULL,
  `fb_id` varchar(200) NOT NULL,
  `name` varchar(200) NOT NULL,
  `comment` text NOT NULL,
  `created_time` varchar(200) NOT NULL,
  `post_id` varchar(200) NOT NULL,
  PRIMARY KEY (`id`),
  UNIQUE KEY `unique_data` (`comment_id`,`fb_id`,`post_id`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1 AUTO_INCREMENT=1 ;
CREATE TABLE IF NOT EXISTS `fb_page_comment_post` (   `id` int(11) NOT NULL AUTO_INCREMENT,   `comment_id` varchar(200) NOT NULL,   `fb_id` varchar(200) NOT NULL,   `name` varchar(200) NOT NULL,   `comment` text NOT NULL,   `created_time` varchar(200) NOT NULL,   `post_id` varchar(200) NOT NULL,   PRIMARY KEY (`id`),   UNIQUE KEY `unique_data` (`comment_id`,`fb_id`,`post_id`) ) ENGINE=InnoDB DEFAULT CHARSET=latin1 AUTO_INCREMENT=1;
