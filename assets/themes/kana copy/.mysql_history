CREATE TABLE IF NOT EXISTS `my_circle` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `userid` int(11) NOT NULL,
  `friendid` int(11) NOT NULL,
  `groupid` int(11) NOT NULL,
  `date_time` datetime NOT NULL,
  `n_status` int(11) NOT NULL,
  PRIMARY KEY (`id`),
  KEY `userid` (`userid`),
  KEY `friendid` (`friendid`),
  KEY `groupid` (`groupid`),
  KEY `n_status` (`n_status`)
) ENGINE=InnoDB  DEFAULT CHARSET=latin1 AUTO_INCREMENT=1 ;
CREATE TABLE IF NOT EXISTS `my_circle` (   `id` int(11) NOT NULL AUTO_INCREMENT,   `userid` int(11) NOT NULL,   `friendid` int(11) NOT NULL,   `groupid` int(11) NOT NULL,   `date_time` datetime NOT NULL,   `n_status` int(11) NOT NULL,   PRIMARY KEY (`id`),   KEY `userid` (`userid`),   KEY `friendid` (`friendid`),   KEY `groupid` (`groupid`),   KEY `n_status` (`n_status`) ) ENGINE=InnoDB  DEFAULT CHARSET=latin1 AUTO_INCREMENT=1;
CREATE TABLE IF NOT EXISTS `my_circle_group` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `userid` int(11) NOT NULL,
  `name` varchar(200) NOT NULL,
  `datetime` datetime NOT NULL,
  `n_status` int(11) NOT NULL,
  PRIMARY KEY (`id`),
  KEY `n_status` (`n_status`),
  KEY `userid` (`userid`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1 AUTO_INCREMENT=1 ;
CREATE TABLE IF NOT EXISTS `my_circle_group` (   `id` int(11) NOT NULL AUTO_INCREMENT,   `userid` int(11) NOT NULL,   `name` varchar(200) NOT NULL,   `datetime` datetime NOT NULL,   `n_status` int(11) NOT NULL,   PRIMARY KEY (`id`),   KEY `n_status` (`n_status`),   KEY `userid` (`userid`) ) ENGINE=InnoDB DEFAULT CHARSET=latin1 AUTO_INCREMENT=1;
