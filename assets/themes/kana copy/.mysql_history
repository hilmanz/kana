DROP TABLE IF EXISTS `channel_subscribers`;
CREATE TABLE IF NOT EXISTS `channel_subscribers` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `channel_name` varchar(100) NOT NULL,
  `channel_id` varchar(100) NOT NULL,
  `subscribe` bigint(20) NOT NULL,
  `date` date NOT NULL,
  `date_ts` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
  PRIMARY KEY (`id`),
  UNIQUE KEY `date` (`date`)
) ENGINE=InnoDB  DEFAULT CHARSET=latin1 AUTO_INCREMENT=30 ;
CREATE TABLE IF NOT EXISTS `channel_subscribers` (   `id` int(11) NOT NULL AUTO_INCREMENT,   `channel_name` varchar(100) NOT NULL,   `channel_id` varchar(100) NOT NULL,   `subscribe` bigint(20) NOT NULL,   `date` date NOT NULL,   `date_ts` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,   PRIMARY KEY (`id`),   UNIQUE KEY `date` (`date`) ) ENGINE=InnoDB  DEFAULT CHARSET=latin1 AUTO_INCREMENT=30;
