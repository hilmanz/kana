TRUNCATE gm_user;
CREATE TABLE IF NOT EXISTS `gm_user` (
  `userID` int(11) NOT NULL AUTO_INCREMENT,
  `username` varchar(20) NOT NULL DEFAULT '',
  `password` varchar(32) NOT NULL DEFAULT '',
  `enc_key` varchar(32) NOT NULL DEFAULT '',
  PRIMARY KEY (`userID`),
  UNIQUE KEY `username` (`username`)
) ENGINE=MyISAM  DEFAULT CHARSET=latin1 AUTO_INCREMENT=10 ;
CREATE TABLE IF NOT EXISTS `gm_user` (   `userID` int(11) NOT NULL AUTO_INCREMENT,   `username` varchar(20) NOT NULL DEFAULT '',   `password` varchar(32) NOT NULL DEFAULT '',   `enc_key` varchar(32) NOT NULL DEFAULT '',   PRIMARY KEY (`userID`),   UNIQUE KEY `username` (`username`) ) ENGINE=MyISAM  DEFAULT CHARSET=latin1 AUTO_INCREMENT=10;
