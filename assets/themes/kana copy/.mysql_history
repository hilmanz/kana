CREATE TABLE IF NOT EXISTS `tbl_activity_actions` (
  `id` int(4) NOT NULL auto_increment,
  `activityName` varchar(64) default NULL,
  PRIMARY KEY  (`id`),
  UNIQUE KEY `activityName` (`activityName`)
) ENGINE=MyISAM  DEFAULT CHARSET=latin1 AUTO_INCREMENT=5 ;
CREATE TABLE IF NOT EXISTS `tbl_activity_actions` (   `id` int(4) NOT NULL auto_increment,   `activityName` varchar(64) default NULL,   PRIMARY KEY  (`id`),   UNIQUE KEY `activityName` (`activityName`) ) ENGINE=MyISAM  DEFAULT CHARSET=latin1 AUTO_INCREMENT=5;
INSERT INTO `tbl_activity_actions` (`id`, `activityName`) VALUES
(1, 'login'),
(2, 'upload_foto'),
(3, 'download'),
(4, 'logout'),
CREATE TABLE IF NOT EXISTS `tbl_activity_log` (
  `id` bigint(21) NOT NULL auto_increment,
  `user_id` bigint(21) NOT NULL,
  `date_ts` int(11) default '0',
  `date_time` datetime default NULL,
  `action_id` int(4) default '0',
  `action_value` varchar(140) default NULL,
  PRIMARY KEY  (`id`),
  UNIQUE KEY `unique_log_id` (`user_id`,`date_ts`,`date_time`),
  KEY `user_id` (`user_id`),
  KEY `actions` (`action_id`,`action_value`),
  KEY `date_ts` (`date_ts`)
) ENGINE=MyISAM  DEFAULT CHARSET=latin1 AUTO_INCREMENT=1 ;
