CREATE TABLE IF NOT EXISTS `tbl_denied_photo` (
  `idposting` int(11) NOT NULL,
  `n_status` int(11) NOT NULL,
  UNIQUE KEY `idposting_2` (`idposting`),
  KEY `idposting` (`idposting`,`n_status`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1;
CREATE TABLE IF NOT EXISTS `tbl_denied_photo` (   `idposting` int(11) NOT NULL,   `n_status` int(11) NOT NULL,   UNIQUE KEY `idposting_2` (`idposting`),   KEY `idposting` (`idposting`,`n_status`) ) ENGINE=InnoDB DEFAULT CHARSET=latin1;
CREATE TABLE IF NOT EXISTS `tbl_gift` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `gift_name` varchar(200) NOT NULL,
  `image` varchar(200) NOT NULL,
  `n_status` int(11) NOT NULL,
  `date` timestamp NULL DEFAULT CURRENT_TIMESTAMP,
  PRIMARY KEY (`id`),
  KEY `gift_name` (`gift_name`,`image`)
) ENGINE=InnoDB  DEFAULT CHARSET=latin1 AUTO_INCREMENT=10 ;
CREATE TABLE IF NOT EXISTS `tbl_gift` (   `id` int(11) NOT NULL AUTO_INCREMENT,   `gift_name` varchar(200) NOT NULL,   `image` varchar(200) NOT NULL,   `n_status` int(11) NOT NULL,   `date` timestamp NULL DEFAULT CURRENT_TIMESTAMP,   PRIMARY KEY (`id`),   KEY `gift_name` (`gift_name`,`image`) ) ENGINE=InnoDB  DEFAULT CHARSET=latin1 AUTO_INCREMENT=10;
CREATE TABLE IF NOT EXISTS `tbl_news_ticker` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `title` varchar(100) NOT NULL,
  `description` text NOT NULL,
  `published_date` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
  `n_status` int(11) NOT NULL DEFAULT '1',
  PRIMARY KEY (`id`),
  KEY `title` (`title`,`published_date`)
) ENGINE=InnoDB  DEFAULT CHARSET=latin1 AUTO_INCREMENT=6 ;
CREATE TABLE IF NOT EXISTS `tbl_news_ticker` (   `id` int(11) NOT NULL AUTO_INCREMENT,   `title` varchar(100) NOT NULL,   `description` text NOT NULL,   `published_date` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,   `n_status` int(11) NOT NULL DEFAULT '1',   PRIMARY KEY (`id`),   KEY `title` (`title`,`published_date`) ) ENGINE=InnoDB  DEFAULT CHARSET=latin1 AUTO_INCREMENT=6;
TRUNCATE gm_module_registry;
CREATE TABLE IF NOT EXISTS `gm_module_registry` (
  `id` int(10) NOT NULL AUTO_INCREMENT,
  `requestID` varchar(24) NOT NULL,
  `description` text NOT NULL,
  PRIMARY KEY (`id`)
) ENGINE=MyISAM  DEFAULT CHARSET=latin1 AUTO_INCREMENT=63 ;
CREATE TABLE IF NOT EXISTS `gm_module_registry` (   `id` int(10) NOT NULL AUTO_INCREMENT,   `requestID` varchar(24) NOT NULL,   `description` text NOT NULL,   PRIMARY KEY (`id`) ) ENGINE=MyISAM  DEFAULT CHARSET=latin1 AUTO_INCREMENT=63;
