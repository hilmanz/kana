DROP TABLE IF EXISTS `tbl_exp_point`;
CREATE TABLE IF NOT EXISTS `tbl_exp_point` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `user_id` int(11) DEFAULT NULL,
  `activity_id` int(11) DEFAULT NULL COMMENT 'tbl_activity_actions',
  `score` int(11) DEFAULT '0',
  `date_time` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP,
  `date_time_ts` varchar(11) DEFAULT NULL,
  PRIMARY KEY (`id`),
  KEY `date_time` (`date_time`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1 AUTO_INCREMENT=1 ;
CREATE TABLE IF NOT EXISTS `tbl_exp_point` (   `id` int(11) NOT NULL AUTO_INCREMENT,   `user_id` int(11) DEFAULT NULL,   `activity_id` int(11) DEFAULT NULL COMMENT 'tbl_activity_actions',   `score` int(11) DEFAULT '0',   `date_time` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP,   `date_time_ts` varchar(11) DEFAULT NULL,   PRIMARY KEY (`id`),   KEY `date_time` (`date_time`) ) ENGINE=InnoDB DEFAULT CHARSET=latin1 AUTO_INCREMENT=1;
