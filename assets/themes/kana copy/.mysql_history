';
truncate axis_news_content;
DROP TABLE IF EXISTS `axis_news_content_type`;
CREATE TABLE IF NOT EXISTS `axis_news_content_type` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `type` varchar(200) NOT NULL,
  `content` int(11) NOT NULL DEFAULT '0' COMMENT '0:content;1:banner',
  `page` varchar(200) NOT NULL COMMENT 'all; page',
  PRIMARY KEY (`id`),
  KEY `category` (`type`),
  KEY `page` (`page`)
) ENGINE=InnoDB  DEFAULT CHARSET=latin1 AUTO_INCREMENT=18 ;
CREATE TABLE IF NOT EXISTS `axis_news_content_type` (   `id` int(11) NOT NULL AUTO_INCREMENT,   `type` varchar(200) NOT NULL,   `content` int(11) NOT NULL DEFAULT '0' COMMENT '0:content;1:banner',   `page` varchar(200) NOT NULL COMMENT 'all; page',   PRIMARY KEY (`id`),   KEY `category` (`type`),   KEY `page` (`page`) ) ENGINE=InnoDB  DEFAULT CHARSET=latin1 AUTO_INCREMENT=18;
truncate axis_news_content_type;
