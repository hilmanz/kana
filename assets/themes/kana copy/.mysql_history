CREATE TABLE IF NOT EXISTS `tbl_user_posting` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `user_id` int(11) NOT NULL,
  `img` text,
  `posting` text NOT NULL,
  `posting_date` datetime NOT NULL,
  `posting_date_ts` int(11) NOT NULL,
  `n_status` int(11) NOT NULL DEFAULT '1',
  PRIMARY KEY (`id`),
  KEY `user_id` (`user_id`,`posting_date`,`posting_date_ts`)
) ENGINE=InnoDB  DEFAULT CHARSET=latin1 AUTO_INCREMENT=22 ;
CREATE TABLE IF NOT EXISTS `tbl_user_posting` (   `id` int(11) NOT NULL AUTO_INCREMENT,   `user_id` int(11) NOT NULL,   `img` text,   `posting` text NOT NULL,   `posting_date` datetime NOT NULL,   `posting_date_ts` int(11) NOT NULL,   `n_status` int(11) NOT NULL DEFAULT '1',   PRIMARY KEY (`id`),   KEY `user_id` (`user_id`,`posting_date`,`posting_date_ts`) ) ENGINE=InnoDB  DEFAULT CHARSET=latin1 AUTO_INCREMENT=22;
