CREATE TABLE `tbl_activity_log_temp` (
  `id` bigint(21) NOT NULL auto_increment,
  `user_id` bigint(21) NOT NULL,
  `date_ts` int(11) default '0',
  `date_time` datetime default NULL,
  `action_id` int(4) default '0',
  `action_value` varchar(140) default NULL,
  `ipaddress` varchar(200) NOT NULL,
  `session` text,
  PRIMARY KEY  (`id`),
  UNIQUE KEY `unique_log_id` (`user_id`,`date_ts`,`date_time`),
  KEY `user_id` (`user_id`),
  KEY `actions` (`action_id`,`action_value`),
  KEY `date_ts` (`date_ts`),
  KEY `ipaddress` (`ipaddress`)
) ENGINE=InnoDB AUTO_INCREMENT=26195 DEFAULT CHARSET=latin1;
CREATE TABLE `tbl_activity_log_temp` (   `id` bigint(21) NOT NULL auto_increment,   `user_id` bigint(21) NOT NULL,   `date_ts` int(11) default '0',   `date_time` datetime default NULL,   `action_id` int(4) default '0',   `action_value` varchar(140) default NULL,   `ipaddress` varchar(200) NOT NULL,   `session` text,   PRIMARY KEY  (`id`),   UNIQUE KEY `unique_log_id` (`user_id`,`date_ts`,`date_time`),   KEY `user_id` (`user_id`),   KEY `actions` (`action_id`,`action_value`),   KEY `date_ts` (`date_ts`),   KEY `ipaddress` (`ipaddress`) ) ENGINE=InnoDB AUTO_INCREMENT=26195 DEFAULT CHARSET=latin1;
show tables;
