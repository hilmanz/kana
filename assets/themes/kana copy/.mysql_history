CREATE TABLE `category_keywords` (
  `id` bigint(21) NOT NULL AUTO_INCREMENT,
  `category_id` int(11) DEFAULT NULL,
  `category_name` varchar(140) DEFAULT NULL,
  `keyword` varchar(140) DEFAULT NULL,
  PRIMARY KEY (`id`),
  UNIQUE KEY `IDX_UNIQUE` (`category_id`,`keyword`),
  KEY `IDX_Keyword` (`keyword`)
) ENGINE=InnoDB AUTO_INCREMENT=40 DEFAULT CHARSET=utf8;
CREATE TABLE `category_keywords` (   `id` bigint(21) NOT NULL AUTO_INCREMENT,   `category_id` int(11) DEFAULT NULL,   `category_name` varchar(140) DEFAULT NULL,   `keyword` varchar(140) DEFAULT NULL,   PRIMARY KEY (`id`),   UNIQUE KEY `IDX_UNIQUE` (`category_id`,`keyword`),   KEY `IDX_Keyword` (`keyword`) ) ENGINE=InnoDB AUTO_INCREMENT=40 DEFAULT CHARSET=utf8;
