DROP TABLE IF EXISTS `gm_dashboard`;
CREATE TABLE IF NOT EXISTS `gm_dashboard` (
  `id` int(10) NOT NULL AUTO_INCREMENT,
  `name` varchar(100) NOT NULL,
  `class` varchar(255) NOT NULL,
  `invoker` varchar(255) NOT NULL,
  `status` tinyint(3) NOT NULL DEFAULT '1',
  `slot` tinyint(3) NOT NULL DEFAULT '1',
  PRIMARY KEY (`id`)
) ENGINE=MyISAM DEFAULT CHARSET=latin1 AUTO_INCREMENT=1 ;
CREATE TABLE IF NOT EXISTS `gm_dashboard` (   `id` int(10) NOT NULL AUTO_INCREMENT,   `name` varchar(100) NOT NULL,   `class` varchar(255) NOT NULL,   `invoker` varchar(255) NOT NULL,   `status` tinyint(3) NOT NULL DEFAULT '1',   `slot` tinyint(3) NOT NULL DEFAULT '1',   PRIMARY KEY (`id`) ) ENGINE=MyISAM DEFAULT CHARSET=latin1 AUTO_INCREMENT=1;
DROP TABLE IF EXISTS `gm_level`;
CREATE TABLE IF NOT EXISTS `gm_level` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `level` varchar(100) NOT NULL,
  `type` varchar(100) NOT NULL,
  `specified_role` int(11) NOT NULL DEFAULT '1',
  `n_status` int(11) NOT NULL DEFAULT '1',
  PRIMARY KEY (`id`),
  KEY `n_status` (`n_status`),
  KEY `specified_role` (`specified_role`)
) ENGINE=InnoDB  DEFAULT CHARSET=latin1 AUTO_INCREMENT=5 ;
CREATE TABLE IF NOT EXISTS `gm_level` (   `id` int(11) NOT NULL AUTO_INCREMENT,   `level` varchar(100) NOT NULL,   `type` varchar(100) NOT NULL,   `specified_role` int(11) NOT NULL DEFAULT '1',   `n_status` int(11) NOT NULL DEFAULT '1',   PRIMARY KEY (`id`),   KEY `n_status` (`n_status`),   KEY `specified_role` (`specified_role`) ) ENGINE=InnoDB  DEFAULT CHARSET=latin1 AUTO_INCREMENT=5;
INSERT INTO `gm_level` (`id`, `level`, `type`, `specified_role`, `n_status`) VALUES
(1, 'admin', 'admin,manager,approver,creator', 0, 1),
(2, 'manager', 'approver,creator', 0, 1),
(3, 'approver', 'approver', 0, 1),
(4, 'creator', 'creator', 1, 1);
INSERT INTO `gm_level` (`id`, `level`, `type`, `specified_role`, `n_status`) VALUES (1, 'admin', 'admin,manager,approver,creator', 0, 1), (2, 'manager', 'approver,creator', 0, 1), (3, 'approver', 'approver', 0, 1), (4, 'creator', 'creator', 1, 1);
DROP TABLE IF EXISTS `gm_member`;
CREATE TABLE IF NOT EXISTS `gm_member` (
  `id` int(10) NOT NULL AUTO_INCREMENT,
  `adminid` int(11) NOT NULL,
  `name` varchar(20) DEFAULT NULL,
  `email` varchar(50) DEFAULT NULL,
  `image` varchar(250) NOT NULL,
  `company` varchar(100) DEFAULT NULL,
  `description` text NOT NULL,
  `n_status` int(11) NOT NULL,
  `articleTypes` text NOT NULL,
  `created_date` datetime NOT NULL,
  PRIMARY KEY (`id`),
  KEY `image` (`image`),
  KEY `contributorid_2` (`adminid`),
  KEY `created_date` (`created_date`),
  KEY `n_status` (`n_status`)
) ENGINE=InnoDB  DEFAULT CHARSET=latin1 AUTO_INCREMENT=9 ;
CREATE TABLE IF NOT EXISTS `gm_member` (   `id` int(10) NOT NULL AUTO_INCREMENT,   `adminid` int(11) NOT NULL,   `name` varchar(20) DEFAULT NULL,   `email` varchar(50) DEFAULT NULL,   `image` varchar(250) NOT NULL,   `company` varchar(100) DEFAULT NULL,   `description` text NOT NULL,   `n_status` int(11) NOT NULL,   `articleTypes` text NOT NULL,   `created_date` datetime NOT NULL,   PRIMARY KEY (`id`),   KEY `image` (`image`),   KEY `contributorid_2` (`adminid`),   KEY `created_date` (`created_date`),   KEY `n_status` (`n_status`) ) ENGINE=InnoDB  DEFAULT CHARSET=latin1 AUTO_INCREMENT=9;
