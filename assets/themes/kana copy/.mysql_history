use athreesix_web_2013
DROP TABLE IF EXISTS `tbl_activity_actions`;
CREATE TABLE IF NOT EXISTS `tbl_activity_actions` (
  `id` int(4) NOT NULL AUTO_INCREMENT,
  `activityName` varchar(64) DEFAULT NULL,
  `type` varchar(200) NOT NULL,
  `point` int(11) NOT NULL DEFAULT '0',
  PRIMARY KEY (`id`),
  UNIQUE KEY `activityName` (`activityName`),
  KEY `point` (`point`),
  KEY `type` (`type`)
) ENGINE=InnoDB  DEFAULT CHARSET=utf8 AUTO_INCREMENT=52 ;
CREATE TABLE IF NOT EXISTS `tbl_activity_actions` (   `id` int(4) NOT NULL AUTO_INCREMENT,   `activityName` varchar(64) DEFAULT NULL,   `type` varchar(200) NOT NULL,   `point` int(11) NOT NULL DEFAULT '0',   PRIMARY KEY (`id`),   UNIQUE KEY `activityName` (`activityName`),   KEY `point` (`point`),   KEY `type` (`type`) ) ENGINE=InnoDB  DEFAULT CHARSET=utf8 AUTO_INCREMENT=52;
