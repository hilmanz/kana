DROP TABLE IF EXISTS `social_news_content`;
CREATE TABLE IF NOT EXISTS `social_news_content` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `userid` int(11) NOT NULL,
  `url` varchar(200) NOT NULL,
  `title` varchar(200) NOT NULL,
  `brief` varchar(200) NOT NULL,
  `content` text NOT NULL,
  `image` varchar(200) NOT NULL,
  `categoryid` int(11) NOT NULL,
  `articleType` int(11) NOT NULL,
  `created_date` datetime NOT NULL,
  `posted_date` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
  `n_status` int(11) NOT NULL DEFAULT '0',
  PRIMARY KEY (`id`),
  KEY `title` (`title`),
  KEY `brief` (`brief`),
  KEY `categoryid` (`categoryid`),
  KEY `created_date` (`created_date`),
  KEY `posted_date` (`posted_date`),
  KEY `n_status` (`n_status`),
  KEY `articleTypeID` (`articleType`),
  KEY `image` (`image`),
  KEY `parentID` (`userid`),
  KEY `url` (`url`)
) ENGINE=InnoDB  DEFAULT CHARSET=latin1 AUTO_INCREMENT=1;
CREATE TABLE IF NOT EXISTS `social_news_content` (   `id` int(11) NOT NULL AUTO_INCREMENT,   `userid` int(11) NOT NULL,   `url` varchar(200) NOT NULL,   `title` varchar(200) NOT NULL,   `brief` varchar(200) NOT NULL,   `content` text NOT NULL,   `image` varchar(200) NOT NULL,   `categoryid` int(11) NOT NULL,   `articleType` int(11) NOT NULL,   `created_date` datetime NOT NULL,   `posted_date` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,   `n_status` int(11) NOT NULL DEFAULT '0',   PRIMARY KEY (`id`),   KEY `title` (`title`),   KEY `brief` (`brief`),   KEY `categoryid` (`categoryid`),   KEY `created_date` (`created_date`),   KEY `posted_date` (`posted_date`),   KEY `n_status` (`n_status`),   KEY `articleTypeID` (`articleType`),   KEY `image` (`image`),   KEY `parentID` (`userid`),   KEY `url` (`url`) ) ENGINE=InnoDB  DEFAULT CHARSET=latin1 AUTO_INCREMENT=1;
DROP TABLE IF EXISTS `social_news_content_comment`;
CREATE TABLE IF NOT EXISTS `social_news_content_comment` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `userid` int(11) NOT NULL,
  `contentid` int(11) NOT NULL,
  `comment` text NOT NULL,
  `date` datetime NOT NULL,
  `n_status` int(11) NOT NULL DEFAULT '1',
  PRIMARY KEY (`id`),
  KEY `date` (`date`),
  KEY `userid` (`userid`),
  KEY `contentid` (`contentid`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1 AUTO_INCREMENT=1 ;
CREATE TABLE IF NOT EXISTS `social_news_content_comment` (   `id` int(11) NOT NULL AUTO_INCREMENT,   `userid` int(11) NOT NULL,   `contentid` int(11) NOT NULL,   `comment` text NOT NULL,   `date` datetime NOT NULL,   `n_status` int(11) NOT NULL DEFAULT '1',   PRIMARY KEY (`id`),   KEY `date` (`date`),   KEY `userid` (`userid`),   KEY `contentid` (`contentid`) ) ENGINE=InnoDB DEFAULT CHARSET=latin1 AUTO_INCREMENT=1;
DROP TABLE IF EXISTS `social_news_content_rating`;
CREATE TABLE IF NOT EXISTS `social_news_content_rating` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `userid` int(11) NOT NULL,
  `contentid` int(11) NOT NULL,
  `rate` int(11) NOT NULL,
  `date` datetime NOT NULL,
  `n_status` int(11) NOT NULL DEFAULT '1',
  PRIMARY KEY (`id`),
  UNIQUE KEY `unique_data` (`userid`,`contentid`),
  KEY `rate` (`rate`),
  KEY `date` (`date`)
) ENGINE=InnoDB  DEFAULT CHARSET=latin1 AUTO_INCREMENT=1 ;
CREATE TABLE IF NOT EXISTS `social_news_content_rating` (   `id` int(11) NOT NULL AUTO_INCREMENT,   `userid` int(11) NOT NULL,   `contentid` int(11) NOT NULL,   `rate` int(11) NOT NULL,   `date` datetime NOT NULL,   `n_status` int(11) NOT NULL DEFAULT '1',   PRIMARY KEY (`id`),   UNIQUE KEY `unique_data` (`userid`,`contentid`),   KEY `rate` (`rate`),   KEY `date` (`date`) ) ENGINE=InnoDB  DEFAULT CHARSET=latin1 AUTO_INCREMENT=1;
select * from social_news_content;
select * from social_news_content_rating;
exit;
show tables;
exit;
select * from channel_video_comment limit 10 ;
select * from channel_video_comment ORDER BY  vid DESC limit 10 ;
ALTER TABLE `channel_video_comment` CHANGE `vid` `vid` VARCHAR( 200 ) NOT NULL ;
exit;
truncate channel_video_comment;
select * from channel_video_comment ORDER BY  vid DESC limit 10 ;
exit;
show tables;
select * from social_news_content ;
select * from social_news_content_rating;
