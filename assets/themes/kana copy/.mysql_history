SELECT id,title,brief,image,thumbnail_image,slider_image,posted_date,FILE,url,fromwho,tags,authorid,topcontent,cityid ,articleType,can_save,content
       FROM beat_news_content WHERE articleType IN (4) AND n_status=1 ORDER BY posted_date DESC ,id DESC;
SELECT id,title,brief,image,thumbnail_image,slider_image,posted_date,FILE,url,fromwho,tags,authorid,topcontent,cityid ,articleType,can_save,content        FROM beat_news_content WHERE articleType IN (4) AND n_status=1 ORDER BY posted_date DESC ,id DESC;
SELECT id,title,brief,image,thumbnail_image,slider_image,posted_date,FILE,url,fromwho,tags,authorid,topcontent,cityid ,articleType,can_save,content        FROM beat_news_content WHERE articleType IN (1) AND n_status=1 ORDER BY posted_date DESC ,id DESC;
